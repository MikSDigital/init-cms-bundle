{% extends admin.getTemplate('base_list_field') %}

{% block field %}
    {%- if value is empty -%}
        &nbsp;
    {%- else -%}
        <span class="show-tooltip" data-original-title="{{value|date}}">{{ value|date }}</span>
    {%- endif -%}
{% endblock %}