<div class="btn-group">
    {#<button type="submit" class="btn" onclick="return start_save_page_settings(this);">#}
    {#{{ 'button.apply'|trans({}, admin.translationdomain) }}</button>#}
    {% if object.isDraft() %}
    <a href="{{ admin.generateUrl('review', {id: object.id}) }}" class=" btn btn-mini">
        {{ 'button.review'|trans({}, admin.translationdomain) }}
    </a>
    {% elseif object.isReview() or object.isPublished() %}
        <a href="{{ admin.generateUrl('draft', {id: object.id}) }}" class="btn btn-mini">
            {{ 'button.draft'|trans({}, admin.translationdomain) }}
        </a>
    {% endif %}

    {% if admin.isGranted('PUBLISH') %}
        <a href="{{ admin.generateUrl('publish', {id: object.id}) }}"
           class="btn btn-mini {% if object.isPublished() %}disabled{% endif %}"
           {% if object.isPublished() %}onclick="return false"{% endif %}>
            {{ 'button.publish'|trans({}, admin.translationdomain) }}
        </a>
    {% endif %}

</div>
<p>&nbsp;</p>
<a href="#" class="{% if object.isPublished() %}disabled{% endif %}"
       {% if object.isPublished() %}onclick="return false"{% endif %}>
        {{ 'button.cancel_draft'|trans({}, admin.translationdomain) }}</a>